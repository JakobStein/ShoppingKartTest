    class stockItem
    {
        private float price;
        private string name;
        private int deal1;
        private float deal2;
        

        public stockItem(string name1, float price1)

        {

            name= name1;
            price= price1;
          

        


        }

        public double getPrice()
        {
            return price;
        }

        public void setDeal(int deal_1, float deal_2)

